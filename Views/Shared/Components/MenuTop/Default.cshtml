@model IEnumerable<Eshopp.Models.EF.Category>

@if (Model != null && Model.Any())
{
    <ul class="navbar_menu">
        @foreach (var item in Model)
        {
            var link = item.Alias == "trang-chu" ? "/" : "/" + item.Alias;
            <li><a href="@link">@item.Title</a></li>
        }
    </ul>
}
